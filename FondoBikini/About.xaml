<Window x:Class="FondoBikini.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Sobre Fondo de Bikini" Height="156" Width="300"
        Name="AboutFondoBikini" ResizeMode="NoResize">
    <Grid>
        <Label Content="Version 1.0" Height="28" HorizontalAlignment="Left"
               Margin="80,15,0,0" Name="version" VerticalAlignment="Top"
               Width="75" />
        <Label Content="Build date: April 2015" Height="28" 
               HorizontalAlignment="Left" Margin="80,45,0,0" Name="buildDate" 
               VerticalAlignment="Top" Width="160"/>
        <Button Content="OK" Height="23" HorizontalAlignment="Center" 
                Margin="0,85,0,0" Name="ok" VerticalAlignment="Top" 
                Width="78" Click="ok_Click"/>
    </Grid>
</Window>
